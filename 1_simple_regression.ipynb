   "source": [
    "def generate_data(n=100):\n",
    "    np.random.seed(42)\n",
    "    x = np.linspace(0, 10, n)\n",
    "    noise = np.random.normal(0, 1.0, size=n)\n",
    "    y = 2.5 * x + 3 + noise\n",
    "    return x, y"
   ]
